\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetSimilarityTimingEstimated(\b string\b0  firstWord, \b string\b0  secondWord) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         \b double\b0  firstLength = firstWord.Length;
\par         \b double\b0  secondLength = secondWord.Length;
\par         \b return\b0 
\par             (firstLength * secondLength * windowSize + firstLength * secondLength * windowSize) *
\par             estimatedTimingConstant;
\par     \}
\par     \b return\b0  0.0;
