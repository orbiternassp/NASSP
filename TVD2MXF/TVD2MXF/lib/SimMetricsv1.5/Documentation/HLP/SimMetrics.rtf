\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \pard\plain \keep\ql\fi0\li100\ri0\sb30\sa30\f1\fs16\cf1 firstWord
\par \pard\plain \keep\ql\fi0\li100\ri0\sb30\sa30\f1\fs16\cf1 secondWord
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Returns
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 returns the score of the similarity measure (un-normalised)
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Body Source
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetUnnormalisedSimilarity(\b string\b0  firstWord, \b string\b0  secondWord) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         \b int\b0  n = firstWord.Length;
\par         \b int\b0  m = secondWord.Length;
\par         \b if\b0  (n == 0) \{
\par             \b return\b0  m;
\par         \}
\par         \b if\b0  (m == 0) \{
\par             \b return\b0  n;
\par         \}
\par         \b double\b0 [][] d = \b new\b0  \b double\b0 [n][];
\par         \b for\b0  (\b int\b0  i = 0; i < n; i++) \{
\par             d[i] = \b new\b0  \b double\b0 [m];
\par         \}
\par         \b double\b0  maxSoFar = defaultMismatchScore;
\par         \b for\b0  (\b int\b0  i = 0; i < n; i++) \{
\par             \b double\b0  cost = dCostFunction.GetCost(firstWord, i, secondWord, 0);
\par             \b if\b0  (i == 0) \{
\par                 d[0][0] = MathFunctions.MaxOf3(defaultMismatchScore, -gapCost, cost);
\par             \}
\par             \b else\b0  \{
\par                 d[i][0] = MathFunctions.MaxOf3(defaultMismatchScore, d[i - 1][0] - gapCost, cost);
\par             \}
\par             \b if\b0  (d[i][0] > maxSoFar) \{
\par                 maxSoFar = d[i][0];
\par             \}
\par         \}
\par 
\par         \b for\b0  (\b int\b0  j = 0; j < m; j++) \{
\par             \b double\b0  cost = dCostFunction.GetCost(firstWord, 0, secondWord, j);
\par             \b if\b0  (j == 0) \{
\par                 d[0][0] = MathFunctions.MaxOf3(defaultMismatchScore, -gapCost, cost);
\par             \}
\par             \b else\b0  \{
\par                 d[0][j] = MathFunctions.MaxOf3(defaultMismatchScore, d[0][j - 1] - gapCost, cost);
\par             \}
\par             \b if\b0  (d[0][j] > maxSoFar) \{
\par                 maxSoFar = d[0][j];
\par             \}
\par         \}
\par 
\par         \b for\b0  (\b int\b0  i = 1; i < n; i++) \{
\par             \b for\b0  (\b int\b0  j = 1; j < m; j++) \{
\par                 \b double\b0  cost = dCostFunction.GetCost(firstWord, i, secondWord, j);
\par                 d[i][j] =
\par                     MathFunctions.MaxOf4(defaultMismatchScore, d[i - 1][j] - gapCost, d[i][j - 1] - gapCost,
\par                                          d[i - 1][j - 1] + cost);
\par                 \b if\b0  (d[i][j] > maxSoFar) \{
\par                     maxSoFar = d[i][j];
\par                 \}
\par             \}
\par         \}
\par 
\par         \b return\b0  maxSoFar;
\par     \}
\par     \b return\b0  0.0;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
