\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetSimilarity(\b string\b0  firstWord, \b string\b0  secondWord) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         \b double\b0  needlemanWunch = GetUnnormalisedSimilarity(firstWord, secondWord);
\par         \b double\b0  maxValue = Math.Max(firstWord.Length, secondWord.Length);
\par         \b double\b0  minValue = maxValue;
\par         \b if\b0  (dCostFunction.MaxCost > gapCost) \{
\par             maxValue *= dCostFunction.MaxCost;
\par         \}
\par         \b else\b0  \{
\par             maxValue *= gapCost;
\par         \}
\par         \b if\b0  (dCostFunction.MinCost < gapCost) \{
\par             minValue *= dCostFunction.MinCost;
\par         \}
\par         \b else\b0  \{
\par             minValue *= gapCost;
\par         \}
\par         \b if\b0  (minValue < defaultMismatchScore) \{
\par             maxValue -= minValue;
\par             needlemanWunch -= minValue;
\par         \}
\par         \b if\b0  (maxValue == defaultMismatchScore) \{
\par             \b return\b0  defaultPerfectMatchScore;
\par         \}
\par         \b else\b0  \{
\par             \b return\b0  defaultPerfectMatchScore - needlemanWunch / maxValue;
\par         \}
\par     \}
\par     \b return\b0  defaultMismatchScore;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
