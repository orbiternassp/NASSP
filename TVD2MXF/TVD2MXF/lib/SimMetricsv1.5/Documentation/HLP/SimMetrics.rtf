\par \pard\plain \keep\ql\fi0\li100\ri0\sb30\sa30\f1\fs16\cf1 characterCombinationIndexValue
\par \pard\plain \ql\fi0\li200\ri0\sb15\sa30\f0\fs16\cf1 error level for skip tokens
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Returns
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 collection of tokens
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Body Source
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  Collection<\b string\b0 > Tokenize(\b string\b0  word, \b bool\b0  extended, \b int\b0  tokenLength, \b int\b0  characterCombinationIndexValue) \{
\par     \b if\b0  (!String.IsNullOrEmpty(word)) \{
\par         SuppliedWord = word;
\par         Collection<\b string\b0 > anArray = \b new\b0  Collection<\b string\b0 >();
\par         \b int\b0  wordLength = word.Length;
\par         \b int\b0  maxValue = 0;
\par         \b if\b0  (tokenLength > 0) \{
\par             maxValue = (tokenLength - 1);
\par         \}
\par         StringBuilder testword = \b new\b0  StringBuilder(wordLength + (2 * maxValue));
\par         \b if\b0  (extended) \{
\par             testword.Insert(0, defaultStartPadCharacter, maxValue);
\par         \}
\par         testword.Append(word);
\par         \b if\b0  (extended) \{
\par             testword.Insert(testword.Length, defaultEndPadCharacter, maxValue);
\par         \}
\par 
\par         \i // normal n-gram keys characterCombinationIndex = 0\i0 
\par         \b string\b0  testWordOne = testword.ToString();
\par         \b int\b0  maxLoop;
\par         \b if\b0  (extended) \{
\par             maxLoop = wordLength + maxValue;
\par         \}
\par         \b else\b0  \{
\par             maxLoop = wordLength - tokenLength + 1;
\par         \}
\par         \b for\b0  (\b int\b0  i = 0; i < maxLoop; i++) \{
\par             \b string\b0  testWord = testWordOne.Substring(i, tokenLength);
\par             \b if\b0  (!stopWordHandler.IsWord(testWord)) \{
\par                 anArray.Add(testWord);
\par             \}
\par         \}
\par 
\par         \b if\b0  (characterCombinationIndexValue != 0) \{
\par             \i // special characterCombinationIndex n-gram keys\i0 
\par             testWordOne = testword.ToString();
\par             maxLoop -= 1; \i // have to reduce by 1 as we are skipping a letter\i0 
\par             \b for\b0  (\b int\b0  i = 0; i < maxLoop; i++) \{
\par                 \b string\b0  testWord = testWordOne.Substring(i, maxValue) + testWordOne.Substring(i + tokenLength, 1);
\par                 \b if\b0  (!stopWordHandler.IsWord(testWord)) \{
\par                     \b if\b0  (!anArray.Contains(testWord)) \{
\par                         anArray.Add(testWord);
\par                     \}
\par                 \}
\par             \}
\par         \}
\par         \b return\b0  anArray;
\par     \}
\par     \b return\b0  \b null\b0 ;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
