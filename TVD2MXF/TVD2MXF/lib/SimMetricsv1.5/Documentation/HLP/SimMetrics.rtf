\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetSimilarity(\b string\b0  firstWord, \b string\b0  secondWord) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         \b double\b0  levensteinDistance = GetUnnormalisedSimilarity(firstWord, secondWord);
\par         \b double\b0  maxLen = firstWord.Length;
\par         \b if\b0  (maxLen < secondWord.Length) \{
\par             maxLen = secondWord.Length;
\par         \}
\par         \b if\b0  (maxLen == defaultMismatchScore) \{
\par             \b return\b0  defaultPerfectMatchScore;
\par         \}
\par         \b else\b0  \{
\par             \b return\b0  defaultPerfectMatchScore - levensteinDistance / maxLen;
\par         \}
