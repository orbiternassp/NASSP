\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  Collection<\b string\b0 > Tokenize(\b string\b0  word) \{
\par     Collection<\b string\b0 > returnVect = \b new\b0  Collection<\b string\b0 >();
\par     \b if\b0  (word != \b null\b0 ) \{
\par         \b int\b0  nextGapPos;
\par         \b for\b0  (\b int\b0  curPos = 0; curPos < word.Length; curPos = nextGapPos) \{
\par             \b char\b0  ch = word[curPos];
\par             \b if\b0  (Char.IsWhiteSpace(ch)) \{
\par                 curPos++;
\par             \}
\par             nextGapPos = word.Length;
\par             \b for\b0  (\b int\b0  i = 0; i < delimiters.Length; i++) \{
\par                 \b int\b0  testPos = word.IndexOf(delimiters[i], curPos);
\par                 \b if\b0  (testPos < nextGapPos && testPos != -1) \{
\par                     nextGapPos = testPos;
\par                 \}
\par             \}
\par 
\par             \b string\b0  term = word.Substring(curPos, (nextGapPos) - (curPos));
\par             \b if\b0  (!stopWordHandler.IsWord(term)) \{
\par                 returnVect.Add(term);
\par             \}
\par         \}
\par     \}
\par     \b return\b0  returnVect;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
