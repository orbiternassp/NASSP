{\fs16\up6 k{\footnote GetUnnormalisedSimilarity,SmithWatermanGotohWindowedAffine;SmithWatermanGotohWindowedAffine,GetUnnormalisedSimilarity}} 
SmithWatermanGotohWindowedAffine.GetUnnormalisedSimilarity \pard\plain \keepn\ql\fi0\li0\ri0\sb30\sa0\f0\fs16\cf1 \par \tx0\tx1440{\uldb SmithWatermanGotohWindowedAffine}{\v id_167>MAIN}\tab {\uldb Body Source}{\v id_639>SOURCE}
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 gets the un-normalised similarity measure of the metric for the given strings.
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetUnnormalisedSimilarity(\b string\b0  firstWord, \b string\b0  secondWord);
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Parameters
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \pard\plain \keep\ql\fi0\li100\ri0\sb30\sa30\f1\fs16\cf1 firstWord
\par \pard\plain \keep\ql\fi0\li100\ri0\sb30\sa30\f1\fs16\cf1 secondWord
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Returns
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 returns the score of the similarity measure (un-normalised)
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Body Source
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetUnnormalisedSimilarity(\b string\b0  firstWord, \b string\b0  secondWord) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         \b int\b0  n = firstWord.Length;
\par         \b int\b0  m = secondWord.Length;
\par         \i // check for zero length input\i0 
\par         \b if\b0  (n == 0) \{
\par             \b return\b0  m;
\par         \}
\par         \b if\b0  (m == 0) \{
\par             \b return\b0  n;
\par         \}
\par         \b double\b0 [][] d = \b new\b0  \b double\b0 [n][];
\par         \b for\b0  (\b int\b0  i = 0; i < n; i++) \{
\par             d[i] = \b new\b0  \b double\b0 [m];
\par         \}
\par         \i //process first row and column first as no need to consider previous rows/columns\i0 
\par         \b double\b0  maxSoFar = 0.0;
\par         \b for\b0  (\b int\b0  i = 0; i < n; i++) \{
\par             \i // get the substution cost\i0 
\par             \b double\b0  cost = dCostFunction.GetCost(firstWord, i, secondWord, 0);
\par             \b if\b0  (i == 0) \{
\par                 d[0][0] = Math.Max(defaultMismatchScore, cost);
\par             \}
\par             \b else\b0  \{
\par                 \b double\b0  maxGapCost = defaultMismatchScore;
\par                 \b int\b0  windowStart = i - windowSize;
\par                 \b if\b0  (windowStart < 1) \{
\par                     windowStart = 1;
\par                 \}
\par                 \b for\b0  (\b int\b0  k = windowStart; k < i; k++) \{
\par                     maxGapCost = Math.Max(maxGapCost, d[i - k][0] - gGapFunction.GetCost(firstWord, i - k, i));
\par                 \}
\par 
\par                 d[i][0] = MathFunctions.MaxOf3(defaultMismatchScore, maxGapCost, cost);
\par             \}
\par             \i //update max possible if available\i0 
\par             \b if\b0  (d[i][0] > maxSoFar) \{
\par                 maxSoFar = d[i][0];
\par             \}
\par         \}
\par 
\par         \b for\b0  (\b int\b0  j = 0; j < m; j++) \{
\par             \i // get the substution cost\i0 
\par             \b double\b0  cost = dCostFunction.GetCost(firstWord, 0, secondWord, j);
\par             \b if\b0  (j == 0) \{
\par                 d[0][0] = Math.Max(defaultMismatchScore, cost);
\par             \}
\par             \b else\b0  \{
\par                 \b double\b0  maxGapCost = defaultMismatchScore;
\par                 \b int\b0  windowStart = j - windowSize;
\par                 \b if\b0  (windowStart < 1) \{
\par                     windowStart = 1;
\par                 \}
\par                 \b for\b0  (\b int\b0  k = windowStart; k < j; k++) \{
\par                     maxGapCost = Math.Max(maxGapCost, d[0][j - k] - gGapFunction.GetCost(secondWord, j - k, j));
\par                 \}
\par 
\par                 d[0][j] = MathFunctions.MaxOf3(defaultMismatchScore, maxGapCost, cost);
\par             \}
\par             \i //update max possible if available\i0 
\par             \b if\b0  (d[0][j] > maxSoFar) \{
\par                 maxSoFar = d[0][j];
\par             \}
\par         \}
\par 
\par         \i // cycle through rest of table filling values from the lowest cost value of the three part cost function\i0 
\par         \b for\b0  (\b int\b0  i = 1; i < n; i++) \{
\par             \b for\b0  (\b int\b0  j = 1; j < m; j++) \{
\par                 \i // get the substution cost\i0 
\par                 \b double\b0  cost = dCostFunction.GetCost(firstWord, i, secondWord, j);
\par                 \i // find lowest cost at point from three possible\i0 
\par                 \b double\b0  maxGapCost1 = defaultMismatchScore;
\par                 \b double\b0  maxGapCost2 = defaultMismatchScore;
\par                 \b int\b0  windowStart = i - windowSize;
\par                 \b if\b0  (windowStart < 1) \{
\par                     windowStart = 1;
\par                 \}
\par                 \b for\b0  (\b int\b0  k = windowStart; k < i; k++) \{
\par                     maxGapCost1 = Math.Max(maxGapCost1, d[i - k][j] - gGapFunction.GetCost(firstWord, i - k, i));
\par                 \}
\par 
\par                 windowStart = j - windowSize;
\par                 \b if\b0  (windowStart < 1) \{
\par                     windowStart = 1;
\par                 \}
\par                 \b for\b0  (\b int\b0  k = windowStart; k < j; k++) \{
\par                     maxGapCost2 = Math.Max(maxGapCost2, d[i][j - k] - gGapFunction.GetCost(secondWord, j - k, j));
\par                 \}
\par 
\par                 d[i][j] = MathFunctions.MaxOf4(defaultMismatchScore, maxGapCost1, maxGapCost2, d[i - 1][j - 1] + cost);
\par                 \b if\b0  (d[i][j] > maxSoFar) \{
\par                     maxSoFar = d[i][j];
\par                 \}
\par             \}
\par         \}
\par 
\par         \i // return max value within matrix as holds the maximum edit score\i0 
\par         \b return\b0  maxSoFar;
\par     \}
\par     \b return\b0  0.0;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
