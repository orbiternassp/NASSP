\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetSimilarity(\b string\b0  firstWord, \b string\b0  secondWord) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         Collection<\b string\b0 > firstTokens = tokeniser.Tokenize(firstWord);
\par         Collection<\b string\b0 > secondTokens = tokeniser.Tokenize(secondWord);
\par 
\par         \b double\b0  sumMatches = 0.0;
\par         \b for\b0  (\b int\b0  i = 0; i < firstTokens.Count; i++) \{
\par             \b string\b0  sToken = firstTokens[i];
\par             \b double\b0  maxFound = 0.0;
\par             \b for\b0  (\b int\b0  j = 0; j < secondTokens.Count; j++) \{
\par                 \b string\b0  tToken = secondTokens[j];
\par                 \b double\b0  found = internalStringMetric.GetSimilarity(sToken, tToken);
\par                 \b if\b0  (found > maxFound) \{
\par                     maxFound = found;
\par                 \}
\par             \}
\par             sumMatches += maxFound;
\par         \}
\par         \b return\b0  sumMatches / firstTokens.Count;
\par     \}
\par     \b return\b0  defaultMismatchScore;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
