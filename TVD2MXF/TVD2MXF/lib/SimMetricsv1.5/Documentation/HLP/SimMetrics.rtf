\par \pard\plain \ql\fi0\li200\ri0\sb15\sa30\f0\fs16\cf1 separation distance
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Returns
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 a string buffer of characters from string1 within string2 if they are of a given distance seperation from the position in string1
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Body Source
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b static\b0  StringBuilder GetCommonCharacters(\b string\b0  firstWord, \b string\b0  secondWord, \b int\b0  distanceSep) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         StringBuilder returnCommons = \b new\b0  StringBuilder();
\par         StringBuilder copy = \b new\b0  StringBuilder(secondWord);
\par         \b for\b0  (\b int\b0  i = 0; i < firstWord.Length; i++) \{
\par             \b char\b0  ch = firstWord[i];
\par             \b bool\b0  foundIt = \b false\b0 ;
\par             \b for\b0  (\b int\b0  j = Math.Max(0, i - distanceSep);
\par                  !foundIt && j < Math.Min(i + distanceSep, secondWord.Length);
\par                  j++) \{
\par                 \b if\b0  (copy[j] == ch) \{
\par                     foundIt = \b true\b0 ;
\par                     returnCommons.Append(ch);
\par                     copy[j] = '#';
\par                 \}
\par             \}
\par         \}
\par 
\par         \b return\b0  returnCommons;
\par     \}
\par     \b return\b0  \b null\b0 ;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
