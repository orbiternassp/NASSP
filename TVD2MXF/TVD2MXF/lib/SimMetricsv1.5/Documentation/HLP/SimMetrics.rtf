\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         \b double\b0  bothLengths = secondWord.Length + firstWord.Length;
\par         \b if\b0  (bothLengths > chapmanMeanLengthMaxString) \{
