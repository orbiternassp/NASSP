\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f1\fs18\cf1 \b public\b0  \b double\b0  GetActualSimilarity(Collection<\b string\b0 > firstTokens, Collection<\b string\b0 > secondTokens);
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Description
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 This is GetActualSimilarity, a member of class QGramsDistance.
\par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Body Source
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b double\b0  GetActualSimilarity(Collection<\b string\b0 > firstTokens, Collection<\b string\b0 > secondTokens) \{
\par     Collection<\b string\b0 > allTokens = tokenUtilities.CreateMergedSet(firstTokens, secondTokens);
\par 
\par     \b int\b0  difference = 0;
\par     \b foreach\b0  (\b string\b0  token \b in\b0  allTokens) \{
\par         \b int\b0  matchingQGrams1 = 0;
\par         \b for\b0  (\b int\b0  i = 0; i < firstTokens.Count; i++) \{
\par             \b if\b0  (firstTokens[i].Equals(token)) \{
\par                 matchingQGrams1++;
\par             \}
\par         \}
\par 
\par         \b int\b0  matchingQGrams2 = 0;
\par         \b for\b0  (\b int\b0  i = 0; i < secondTokens.Count; i++) \{
\par             \b if\b0  (secondTokens[i].Equals(token)) \{
\par                 matchingQGrams2++;
\par             \}
\par         \}
\par         \b if\b0  (matchingQGrams1 > matchingQGrams2) \{
\par             difference += matchingQGrams1 - matchingQGrams2;
\par         \}
\par         \b else\b0  \{
\par             difference += matchingQGrams2 - matchingQGrams1;
\par         \}
\par     \}
\par     \b return\b0  difference;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
