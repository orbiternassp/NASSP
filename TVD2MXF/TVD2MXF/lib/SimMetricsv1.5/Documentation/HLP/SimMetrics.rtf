\par d(i,j) is a function whereby d(c,d)=0 if c=d, 1 else. \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Body Source
\par \pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 {\pard\plain \keep\ql\fi0\li0\ri0\sb0\sa0\f1\fs18\cf1 \b public\b0  \b override\b0  \b double\b0  GetUnnormalisedSimilarity(\b string\b0  firstWord, \b string\b0  secondWord) \{
\par     \b if\b0  ((firstWord != \b null\b0 ) && (secondWord != \b null\b0 )) \{
\par         \i // Step 1\i0 
\par         \b int\b0  n = firstWord.Length;
\par         \b int\b0  m = secondWord.Length;
\par         \b if\b0  (n == 0) \{
\par             \b return\b0  m;
\par         \}
\par         \b if\b0  (m == 0) \{
\par             \b return\b0  n;
\par         \}
\par 
\par         \b double\b0 [][] d = \b new\b0  \b double\b0 [n + 1][];
\par         \b for\b0  (\b int\b0  i = 0; i < n + 1; i++) \{
\par             d[i] = \b new\b0  \b double\b0 [m + 1];
\par         \}
\par 
\par         \i // Step 2\i0 
\par         \b for\b0  (\b int\b0  i = 0; i <= n; i++) \{
\par             d[i][0] = i;
\par         \}
\par         \b for\b0  (\b int\b0  j = 0; j <= m; j++) \{
\par             d[0][j] = j;
\par         \}
\par 
\par         \i // Step 3\i0 
\par         \b for\b0  (\b int\b0  i = 1; i <= n; i++) \{
\par             \i // Step 4\i0 
\par             \b for\b0  (\b int\b0  j = 1; j <= m; j++) \{
\par                 \i // Step 5\i0 
\par                 \b double\b0  cost = dCostFunction.GetCost(firstWord, i - 1, secondWord, j - 1);
\par                 \i // Step 6\i0 
\par                 d[i][j] = MathFunctions.MinOf3(d[i - 1][j] + 1.0, d[i][j - 1] + 1.0, d[i - 1][j - 1] + cost);
\par             \}
\par         \}
\par 
\par         \i // Step 7\i0 
\par         \b return\b0  d[n][m];
\par     \}
\par     \b return\b0  0.0;
\par \}
\par }\pard\plain \ql\fi0\li0\ri0\sb30\sa30\f0\fs18\cf1 \par \pard\plain \ql\fi0\li0\ri0\sb60\sa30\f0\fs18\cf1\b Links
